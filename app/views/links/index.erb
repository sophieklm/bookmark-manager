<h2> Links </h2>
<div id='links-container'>
<ul id='links'>
  <% @links.each do |link| %>
  <li>
    Title: <%= link.title %>
    URL: <a class='title' href='<%= link.url %>'><%= link.url %></a>
    Tags:
    <% link.tags.each do |tag| %>
      <%= tag.tag %>
    <%end%>
  </li>
  <% end %>
</ul>
</div>
